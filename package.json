{
  "name": "libmpvjs",
  "version": "0.0.1",
  "description": "libmpv bindings for Electron",
  "main": "index.js",
  "author": "zenwarr",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/zenwarr/libmpvjs"
  },
  "devDependencies": {
    "prebuild": "^6.2.2"
  },
  "dependencies": {
    "prebuild-install": "^2.3.0"
  },
  "bundledDependencies": [
    "prebuild-install"
  ],
  "scripts": {
    "install": "prebuild-install",
    "build-module": "node-gyp configure && node-gyp rebuild --target=1.7.9 --arch=x64 --dist-url=https://atom.io/download/electron",
    "prebuild": "prebuild -t 1.1.0 -t 1.2.0 -t 1.3.0 -t 1.4.0 -t 1.5.0 -t 1.6.0 -t 1.7.0 -t 1.8.0 -r electron --strip"
  },
  "types": "libmpvjs.d.ts"
}
